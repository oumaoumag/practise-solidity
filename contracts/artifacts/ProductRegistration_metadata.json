					"params": {
						"_productId": "Unique identifier of the product"
					},
					"returns": {
						"_0": "Product details"
					}
				},
				"registerProduct(string,uint256,uint256,string,string)": {
					"details": "Registers a new dairy product on the blockchain",
					"params": {
						"_batchInformation": "Batch-specific information",
